PROJECT = clprint

include ../../../Makefile.common

DAO_INCS += -I../../../kernel
DAO_LIBS += -L../../..

TARGET = ../$(PROJECT).$(DAO_DLL_EXT)

all: $(TARGET)

dao_clprint.o: dao_clprint.c
	$(DAO_CC) -c $(DAO_CFLAGS) $(DAO_INCS) dao_clprint.c

$(TARGET): dao_clprint.o
	$(DAO_CC) $(DAO_DLLFLAGS) dao_clprint.o $(DAO_LIBS) $(DAO_LFLAGS_DAO2) -o $(TARGET)

clean:
	-$(DEL_FILE) *.o
